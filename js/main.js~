new TypeWriter(`#typer`, {
    loop: true,
    cursor: {
      speed: 700,
      size: 'larger',
      id: 'Some_TypeWriter_CSS',
      enabled: true
    },
    timeout: 50
  })
  .write(`Developer`)
  .wait(2000)
  .removeAll()
  .write(`Tinkerer`)
  .wait(2000)
  .removeAll()
  .write(`Maker`)
  .wait(2000)
  .removeAll()
  .start();